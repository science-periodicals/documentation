{
  "name": "@scipe/documentation",
  "version": "0.0.0",
  "description": "sci.pe documentation",
  "main": "dist/index.js",
  "style": "src/index.css",
  "scripts": {
    "profile": "cross-env NODE_ENV=production webpack --mode production --profile --json > stats.json && webpack-bundle-analyzer stats.json public/assets",
    "start": "cross-env NODE_ENV=production node dist/server.js",
    "test": "mocha --reporter spec --require @babel/register --exit",
    "watch": "babel-node ./src/server.js",
    "format": "prettier --single-quote --write \"{src,test,example}/**/*.js\"",
    "build-client": "rm -rf ./public/assets/documentation-bundle* && cross-env NODE_ENV=production webpack --mode production --progress",
    "build-server": "rm -rf dist; mkdir dist; babel src -d dist",
    "build": "npm run build-client && npm run build-server",
    "prepare": "npm run build",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/science-periodicals/documentation.git"
  },
  "eslintConfig": {
    "extends": "@scipe"
  },
  "prettier": {
    "singleQuote": true
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": "> 0.25%, not dead"
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "keywords": [
    "documentation"
  ],
  "author": "Sebastien Ballesteros",
  "license": "See README.md",
  "bugs": {
    "url": "https://github.com/science-periodicals/documentation/issues"
  },
  "homepage": "https://github.com/science-periodicals/documentation#readme",
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.0",
    "@babel/node": "^7.5.0",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.5.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "@scipe/eslint-config": "^1.0.0",
    "@scipe/express-logger": "^1.0.0",
    "assets-webpack-plugin": "^3.9.10",
    "babel-loader": "^8.0.6",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "eslint": "^5.16.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "mini-css-extract-plugin": "^0.7.0",
    "mocha": "^6.1.4",
    "normalize.css": "^8.0.1",
    "null-loader": "^3.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.1.2",
    "postcss-preset-env": "^6.6.0",
    "postcss-reporter": "^6.0.1",
    "postcss-url": "^8.0.0",
    "prettier": "^1.18.2",
    "request": "^2.88.0",
    "server-destroy": "^1.0.1",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.3.0",
    "url-loader": "^2.0.1",
    "webpack": "^4.35.2",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.5",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-dev-server": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@babel/runtime": "^7.5.0",
    "@scipe/iconoclass": "^1.0.0",
    "@scipe/jsonld": "^1.0.0",
    "@scipe/ontology": "^23.0.0",
    "@scipe/resources": "^1.0.0",
    "@scipe/ui": "^1.0.0",
    "classnames": "^2.2.6",
    "d3": "^3.5.17",
    "decamelize": "^1.2.0",
    "ejs": "^2.6.2",
    "express": "^4.17.1",
    "highlight.js": "^9.15.8",
    "jsonld-markup": "^0.2.1",
    "jsonld-vis": "^2.1.0",
    "marked": "^0.6.3",
    "param-case": "^2.1.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet-async": "^1.0.2",
    "react-router-dom": "^5.0.1",
    "react-scroll": "^1.7.12",
    "schema.org": "^3.1.1",
    "smoothscroll-polyfill": "^0.4.4",
    "uuid": "^3.3.2"
  }
}
